{
  "name": "w3xdata",
  "version": "2.2.0",
  "description": "Exact data from Warcraft III maps",
  "author": "verit",
  "license": "MIT",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "test-debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test": "jest",
    "sandbox-debug": "node -r ts-node/register --inspect-brk src/sandbox.ts",
    "sandbox": "ts-node src/sandbox.ts"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.16",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "eslint-config-verit": "^3.3.0",
    "eslint-plugin-prettier": "^3.3.0",
    "jest": "^26.6.3",
    "npm-watch": "^0.7.0",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts",
      "legacyWatch": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voces/w3xdata.git"
  },
  "bugs": {
    "url": "https://github.com/voces/w3xdata/issues"
  },
  "homepage": "https://github.com/voces/w3xdata#readme",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "dependencies": {
    "mdx-m3-viewer": "^5.1.3",
    "wc3data": "^2.2.2"
  }
}
